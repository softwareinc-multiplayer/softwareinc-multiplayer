{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "src": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "obj/api"
    },
    {
      "src": [
        {
          "files": [
            "Assembly-CSharp.dll"
          ],
          "src": "../resources/docs/game"
        }
      ],
      "dest": "obj/game",
      "filter": "docfxFilter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "game/**.yml"
        ],
        "src": "obj"
      },
      {
        "files": [
          "api/*.md",
          "game/*.md",
          "toc.yml",
          "*.md"
        ],
        "src": "../resources/docs"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "../resources/docs"
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "src": "../resources/docs"
      }
    ],
    "dest": "../docs",
    "template": [
      "statictoc"
    ],
    "globalMetadata": {
      "_disableContribution": true
    }
  }
}
